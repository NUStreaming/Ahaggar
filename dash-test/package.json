{
    "name": "dash-test-batch",
    "version": "1.0.0",
    "description": "dash-test-batch",
    "dependencies": {
        "express": "^4.18.1",
        "http-proxy-middleware": "^2.0.6",
        "puppeteer": "^5.5.0",
        "puppeteer-core": "^5.5.0"
    },
    "config": {
        "client_profile": "./client_profile_join_test_combined",
        "network_profile": "4G-Lumous",
        "num_clients": 5,
        "abr_strategy": "abrAhaggar",
        "batchTest_enabled": false,
        "batchTest_comments": "[batchTest]"
    },
    "scripts": {
        "test": "node run.js $npm_package_config_network_profile || true",
        "test-multiple-clients": "sudo rm -rf tmpUserDataDir && (docker stop $(docker ps -q --filter ancestor=dash-test) || true) && docker build -t dash-test . && node run-multiple-clients.js $npm_package_config_network_profile || true"
    }
}
